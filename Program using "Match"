def makematch():
    print("Please choose from A:Multiply / B:Divide / C:Add / D:Subtract / E:Remainder")
    option = input("Please enter your choice")
    num1 = int(input("Please enter your first number: "))
    num2 = int(input("Please enter your second number: "))
    match option:
        case "A":
            print(num1 * num2)
            
        case "B":
            print(num1 / num2)
        case "C":
            print(num1 + num2)
        case "D":
            print(num1 - num2)
        case "E":
            print(num1 % num2)

    return option

answer=makematch()

print(answer)
